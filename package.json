{
  "name": "cdk-athena-workgroup",
  "description": "CDK Construct for creating Athena WorkGroups",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Daniel Schroeder",
    "url": "https://www.udondan.com/",
    "twitter": "udondan"
  },
  "awscdkio": {
    "twitter": "udondan"
  },
  "homepage": "https://github.com/udondan/cdk-athena-workgroup",
  "repository": {
    "type": "git",
    "url": "https://github.com/udondan/cdk-athena-workgroup.git"
  },
  "bugs": {
    "url": "https://github.com/udondan/cdk-athena-workgroup/issues"
  },
  "keywords": [
    "aws",
    "cdk",
    "aws-cdk",
    "athena",
    "workgroup"
  ],
  "stability": "stable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "prepackage": "jsii && lambda/build"
  },
  "jsii": {
    "excludeTypescript": [
      "lambda"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-athena-workgroup",
        "module": "cdk_athena_workgroup"
      },
      "dotnet": {
        "namespace": "CDK.Athena.WorkGroup",
        "packageId": "CDK.Athena.WorkGroup"
      }
    }
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.50.0",
    "@types/aws-lambda": "^8.10.58",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^10.17.5",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.711.0",
    "jsii": "^1.5.0",
    "jsii-pacmak": "^1.5.0",
    "ts-node": "^8.10.2",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "^1.50.0",
    "@aws-cdk/aws-iam": "^1.50.0",
    "@aws-cdk/aws-lambda": "^1.50.0",
    "@aws-cdk/core": "^1.50.0",
    "iam-floyd": "^0.20.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-cloudformation": "^1.50.0",
    "@aws-cdk/aws-iam": "^1.50.0",
    "@aws-cdk/aws-lambda": "^1.50.0",
    "@aws-cdk/core": "^1.50.0",
    "constructs": "^3.0.4",
    "iam-floyd": "^0.20.0"
  }
}
